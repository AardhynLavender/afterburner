{
  "name": "afterburner",
  "version": "1.0.0",
  "main": "App.tsx",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "start": "expo start --tunnel",
    "start:android": "expo start --android --tunnel",
    "start:ios": "expo start --ios --tunnel",
    "start:web": "expo start --web --tunnel",
    "build": "eas build --platform all",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "env:push": "eas secret:push --scope project --env-file ./.env --force",
    "deploy:rules": "firebase deploy --only firestore:rules && firebase deploy --only storage",
    "deploy:hosting": "yarn build && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "firebase:init": "yarn firebase:login && firebase init",
    "firebase:login": "firebase login",
    "firebase:logout": "firebase logout",
    "emulate": "yarn emulate:start",
    "emulate:start": "firebase emulators:start",
    "emulate:stop": "firebase emulators:stop",
    "internal:android": "npx expo install --check && npx expo-doctor && npx eas build -p android --profile preview",
    "logging:android": "adb logcat *:E"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.1.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/datetimepicker": "7.2.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^18.3.2",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/material-top-tabs": "^6.6.4",
    "@react-navigation/stack": "^6.3.17",
    "@tanstack/react-query": "^4.32.6",
    "expo": "^49.0.8",
    "expo-app-loading": "^1.0.3",
    "expo-av": "~13.4.1",
    "expo-barcode-scanner": "~12.5.3",
    "expo-camera": "~13.4.4",
    "expo-font": "~11.4.0",
    "expo-image": "~1.3.4",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.14",
    "firebase": "^10.3.1",
    "firebase-tools": "^12.5.2",
    "fix": "^0.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-native": "0.72.5",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-pager-view": "6.2.0",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "~3.22.0",
    "react-native-select-dropdown": "^3.4.0",
    "react-native-svg": "13.9.0",
    "react-native-tab-view": "^3.5.2",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-vector-icons": "^10.0.0",
    "react-native-web": "~0.19.6",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native-vector-icons": "^6.4.13",
    "typescript": "^5.1.3"
  },
  "private": true
}
